      "enum": [
        "self",
        "user"
      ]
    },
    "quantity": {
      "type": "integer"
    },
    "allocatedQuantity": {
      "type": "integer"
    },
    "unityWheight": {
      "type": "decimal"
    },
    "totalQuantity": {
      "type": "decimal"
    },
    "service": {
      "type": "enumeration",
      "enum": [
        "self",
        "user"
      ]
    },
    "images": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": true
    },
    "testVideo": {
      "allowedTypes": [
        "videos"
      ],
      "type": "media",
      "multiple": true,
      "required": false
