    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "uuid": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "entryCode": {
      "type": "string"
    },
    "entryDate": {
      "type": "datetime"
    },
    "origin": {
